# Frontend Environment Variables
# Copy this file to .env.local and modify as needed

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_WS_URL=http://localhost:3001

# Windows Backend URLs (for development)
VITE_WINDOWS_API_URL=http://YOUR_WINDOWS_IP:3001/api
VITE_WINDOWS_WS_URL=http://YOUR_WINDOWS_IP:3001

# Production URLs (replace with your actual production URLs)
VITE_PRODUCTION_API_URL=https://your-production-api.com/api
VITE_PRODUCTION_WS_URL=https://your-production-api.com

# App Environment
VITE_APP_ENV=development

# Security (these should be set in your deployment platform)
VITE_APP_SECRET_KEY=your-app-secret-key
VITE_ENCRYPTION_KEY=your-encryption-key

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
